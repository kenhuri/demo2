<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Poste.
    -->
    <changeSet id="20180718134223-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="place_id"
                                 baseTableName="poste"
                                 constraintName="fk_poste_place_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bureau"/>

        <addForeignKeyConstraint baseColumnNames="habilitation_id"
                                 baseTableName="poste"
                                 constraintName="fk_poste_habilitation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personne"/>

    </changeSet>
</databaseChangeLog>
